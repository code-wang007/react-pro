{
  "env": {
    "browser": true
  },
  "extends": ["airbnb"],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "react",
    "@typescript-eslint"
  ],
  "rules": {
    // "parser": "babel-eslint",
    "react/prefer-stateless-function": "off",
